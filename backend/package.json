{
  "name": "nomotix-backend",
  "version": "1.0.0",
  "description": "Nomotix - Premium e-commerce platform backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "node scripts/seed.js",
    "seed:custom": "node scripts/addCustomProducts.js",
    "seed:more": "node scripts/addMoreProducts.js",
    "seed:reviews": "node scripts/addReviewsAndOrders.js",
    "seed:all": "npm run seed && npm run seed:custom && npm run seed:more && npm run seed:reviews",
    "seed:400": "node scripts/generate400Products.js && node scripts/addReviewsAndOrders.js",
    "fix:images": "node scripts/fixProductImages.js"
  },
  "keywords": [
    "ecommerce",
    "mern",
    "express",
    "mongodb",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.23.12",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lottie-react": "^2.4.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "react-intersection-observer": "^9.16.0",
    "stripe": "^14.7.0",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  }
}
